class Solution {
    public int lcs(int[]nums,int[]nums2) {
        int n = nums.length;
        int m = nums2.length;
        int dp[][] = new int[n+1][m+1];

        // initialise
        for(int i=0;i<n+1;i++) {
            dp[i][0] = 0;
        }

        for(int j=0;j<m+1;j++) {
            dp[0][j] = 0;
        }

        // code
        for(int i=1;i<n+1;i++) {
            for(int j=1;j<m+1;j++) {
                if(nums[i-1] == nums2[j-1]) {
                    dp[i][j] = dp[i-1][j-1] + 1;
                }
                else{
                    dp[i][j] = Math.max(dp[i-1][j] , dp[i][j-1]);
                }
            }
        }
        return dp[n][m];
    }

    public int lengthOfLIS(int[] nums) {
        int n = nums.length;
        HashSet<Integer> hs = new HashSet<>();
        for(int i=0;i<n;i++) {
            hs.add(nums[i]);
        }
        int[] nums2 = new int[hs.size()];
        int i=0;
        for(int val : hs) {
            nums2[i++] = val;
        }
        Arrays.sort(nums2);
        return lcs(nums,nums2);
    }
}
