class Solution {
    public int lengthOfLongestSubstring(String s) {
        int n = s.length();
        int len=0;
        int maxlength = 0;
        for(int i=0;i<n;i++) {
            HashSet<Character> hs = new HashSet<>();
            for(int j=i;j<n;j++) {
                if(hs.contains(s.charAt(j))) break;
                hs.add(s.charAt(j));
                maxlength = Math.max(maxlength,j-i+1);
            }
        }
        return maxlength;
    }
}
