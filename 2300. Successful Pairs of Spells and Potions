class Solution {
    public int[] successfulPairs(int[] spells, int[] potions, long success) {
        int n = spells.length;
        int m = potions.length;
        int res[] = new int[n];
        Arrays.sort(potions);
        for(int i=0;i<n;i++) {
        int count = 0;
            int start=0;
            int end = m-1;
            while(start<=end) {
                int mid = start+(end-start)/2;
                long prod = (long) spells[i] * potions[mid];
                if(prod>=success) {
                    end = mid-1;
                    count=m-mid;
                }
                else{
                    start = mid+1;

                } 
            }
            res[i]+=count;
        }
        return res;
    }
}
