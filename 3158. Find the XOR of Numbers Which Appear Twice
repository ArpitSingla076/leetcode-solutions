class Solution {
    public int duplicateNumbersXOR(int[] nums) {
        Arrays.sort(nums);
        int n = nums.length;
        int result = 0;
        for(int i=0;i<n-1;i++) {
            if(nums[i]==nums[i+1]) {
                result^=nums[i];
            }
        }
        return result;
    }
}
