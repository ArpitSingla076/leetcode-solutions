class Solution {
    public int findContentChildren(int[] g, int[] s) {
        int m = g.length;
        int n = s.length;
        Arrays.sort(g);
        Arrays.sort(s);
        int i=0;
        int j=0;
        int count=0;
        while(i<m && j<n) {
            // if it can satisfy
            if(s[j]>=g[i]) {
                i++;
                j++;
                count++;
            }
            else{
                j++;
            }
        }
        return count;
    }
}
