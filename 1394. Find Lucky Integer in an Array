class Solution {
    public int findLucky(int[] arr) {
        HashMap<Integer,Integer> map = new HashMap<>();
        int n = arr.length;
        for(int i=0;i<n;i++) {
            int num = arr[i];
            if(map.containsKey(num)) {
                map.put(num,map.get(num)+1);
            }
            else{
                map.put(num,1);
            }
        }

        int maxkey=-1;
        for(int key : map.keySet()) {
            if(map.get(key) == key) {
                maxkey = Math.max(key,maxkey);
            }    
        }
        return maxkey;
    }
}
